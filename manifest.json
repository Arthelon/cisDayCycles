{
  "name": "CIS Day Cycles",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A browser extension to remind CISHK students/faculty of timetable day cycles",
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "CIS Day Cycles"
  },
  "chrome_url_overrides": {
    "newtab": "override.html"
  }
}


